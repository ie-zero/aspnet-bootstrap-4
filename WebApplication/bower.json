{
  "name": "WebApplication",
  "private": true,
  "dependencies": {
    "bootstrap": "4.0.0-alpha",
    "jquery": "2.1.4",
    "jquery-validation": "1.14.0",
    "jquery-validation-unobtrusive": "3.2.2"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/css/*.**",
        "dist/js/*.**"
      ]
    },
    "jquery": {
      "main": "dist/jquery{.js,.min.js,.min.map}"
    },
    "jquery-validation": {
      "main": "dist/jquery.validate{.js,.min.js}"
    },
    "jquery-validation-unobtrusive": {
      "main": "jquery.validate.unobtrusive{.js,.min.js}"
    }
  }
}
